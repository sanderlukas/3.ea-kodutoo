{
  "name": "Sizer",
  "description": "Get your font size just as you like",
  "version": "1",
  "permissions": ["activeTab", "declarativeContent"],
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}